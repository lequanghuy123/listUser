<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>manageuser.controllers.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>listUser</servlet-name>
		<servlet-class>manageuser.controllers.ListUserController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>manageuser.controllers.LogoutController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>listUser</servlet-name>
		<url-pattern>/listUser.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout.do</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>/view/jsp/ADM001.jsp</welcome-file>
	</welcome-file-list>
</web-app>